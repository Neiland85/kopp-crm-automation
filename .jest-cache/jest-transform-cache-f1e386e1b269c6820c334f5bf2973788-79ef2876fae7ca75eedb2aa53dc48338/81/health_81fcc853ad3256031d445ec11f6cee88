 *       503:
 *         description: Uno o mÃ¡s servicios no disponibles
 *         content:
 *           application/json:
 *             schema:
 *               $ref: '#/components/schemas/HealthCheck'
 *             example:
 *               status: "error"
 *               timestamp: "2024-06-29T18:00:00.000Z"
