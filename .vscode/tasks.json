    {
      "label": "ğŸ› ï¸ Setup Completo del Workspace",
      "type": "shell",
      "command": "npm",
      "args": ["run", "setup:all"],
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "new",
        "focus": true
      },
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "ğŸ§ª Ejecutar Tests",
      "type": "shell",
      "command": "npm",
      "args": ["test"],
      "group": "test",
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": ["$eslint-stylish"],
      "isBackground": false
    },
    {
      "label": "ğŸ§ª Tests en Modo Watch",
      "type": "shell",
      "command": "npm",
      "args": ["run", "test:watch"],
      "group": "test",
      "presentation": {
        "reveal": "always",
        "panel": "dedicated"
      },
      "isBackground": true,
      "problemMatcher": {
        "owner": "jest",
        "source": "jest",
        "fileLocation": "relative",
        "pattern": [
          {
            "regexp": "^\\s*(â—|âœ•)\\s+(.*)$",
            "message": 2
          },
          {
            "regexp": "^\\s+at\\s+.*\\((.*):(\\d+):(\\d+)\\)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "loop": true
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^\\s*\\$\\s+jest --watch",
          "endsPattern": "(^\\s*Watch Usage$|^\\s*press .* to show more\\.$)"
        }
      }
    },
